import pandas as pd


# Simple Series 1 D

s1 = pd.Series([2, 4, 6, 8, 10])


type(s1)


print(s1)


# Series with Custom Index
s2 = pd.Series([2, 4, 5, 7, 8, 9], index=["Dushyant","Mayank","Divyansh","Drishti","Kriti","Neeti"])


s2


print(s2["Dushyant"])


print(s2["Neeti"])


